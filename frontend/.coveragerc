[run]
source = src
omit = 
    src/test/*
    src/**/*.test.js
    src/**/*.test.jsx
    src/**/*.stories.js
    src/**/*.stories.jsx
    src/main.jsx
    src/**/*.config.js

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:

[html]
directory = coverage/html

[xml]
output = coverage/coverage.xml